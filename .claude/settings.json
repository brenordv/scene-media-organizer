  "secretDetection": {
    "enabled": true,
    "patterns": [
      "(?i)(api[_-]?key|apikey)\\s*[=:]\\s*['\"]?[a-zA-Z0-9_\\-]{20,}['\"]?",
      "(?i)(secret[_-]?key|secretkey)\\s*[=:]\\s*['\"]?[a-zA-Z0-9_\\-]{20,}['\"]?",
      "(?i)(access[_-]?token|accesstoken)\\s*[=:]\\s*['\"]?[a-zA-Z0-9_\\-]{20,}['\"]?",
      "(?i)(auth[_-]?token|authtoken)\\s*[=:]\\s*['\"]?[a-zA-Z0-9_\\-]{20,}['\"]?",
      "(?i)(password|passwd|pwd)\\s*[=:]\\s*['\"]?[^\\s'\"]{8,}['\"]?",
      "-----BEGIN (RSA|OPENSSH|DSA|EC|PGP) PRIVATE KEY-----",
      "(?i)aws_access_key_id\\s*[=:]\\s*['\"]?[A-Z0-9]{20}['\"]?",
      "(?i)aws_secret_access_key\\s*[=:]\\s*['\"]?[A-Za-z0-9/+=]{40}['\"]?",
      "(?i)github[_-]?token\\s*[=:]\\s*['\"]?gh[pousr]_[A-Za-z0-9_]{36,}['\"]?",
      "(?i)sk-[a-zA-Z0-9]{48}",
      "(?i)sk-ant-[a-zA-Z0-9\\-_]{95,}",
      "(?i)AKIA[0-9A-Z]{16}",
      "(?i)AIza[0-9A-Za-z\\-_]{35}",
      "(?i)ya29\\.[0-9A-Za-z\\-_]+",
      "(?i)[0-9]+-[0-9A-Za-z_]{32}\\.apps\\.googleusercontent\\.com",
      "(?i)sq0csp-[0-9A-Za-z\\-_]{43}",
      "(?i)stripe[_-]?[a-z]*[_-]?key\\s*[=:]\\s*['\"]?[rs]k_(live|test)_[0-9a-zA-Z]{24,}['\"]?"
    ],
    "scanOnRead": true,
    "scanOnWrite": false,
    "action": "warn",
    "excludePatterns": [
      "**/*.env.example",
      "**/*.env.template",
      "**/*.sample",
      "**/README.md",
      "**/SECURITY.md"
    ]
  },
